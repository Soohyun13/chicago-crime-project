X_val, y_tr, y_val = train_test_split(X_train, y_train, test_size=0.2, random_state=42)\n","\n","# ✅ 최적화 함수 정의\n","def objective(trial):\n","    params = {\n","        'max_depth': trial.suggest_int('max_depth', 3, 10),\n","        'learning_rate': trial.suggest_float('learning_rate', 0.01, 0.2),\n","        'n_estimators': trial.suggest_int('n_estimators', 100, 400),\n","        'subsample': trial.suggest_float('subsample', 0.6, 1.0),\n","        'colsample_bytree': trial.suggest_float('colsample_bytree', 0.6, 1.0),\n","        'gamma': trial.suggest_float('gamma', 0, 2),\n","        'min_child_weight': trial.suggest_int('min_child_weight', 1, 10),\n","        'scale_pos_weight': trial.suggest_categorical('scale_pos_weight', [1, 5, 10]),\n","        'objective': 'binary:logistic',\n","        'eval_metric': 'logloss',\n","        'tree_method': 'hist',\n","        'use_label_encoder': False,\n","        'random_state': 42,\n","    }\n","\n","    model = xgb.XGBClassifier(**params)\n","    model.fit(X_tr, y_tr)\n","    preds = model.predict(X_val)\n","    score = f1_score(y_val, preds)\n","    return score  # maximize\n","\n","# ✅ 스터디 시작\n","study = optuna.create_study(direction='maximize')\n","study.optimize(objective, n_trials=30)\n","\n","# ✅ 결과 출력\n","print(\"🎯 Best F1 Score:\", study.best_value)\n","print(\"🏆 Best Params:\", study.best_params)\n","\n","# ✅ 최적 파라미터로 모델 훈련\n","best_params = study.best_params\n","best_params.update({\n","    'objective': 'binary:logistic',\n","    'eval_metric': 'logloss',\n","    'tree_method': 'hist',\n","    'use_label_encoder': False,\n","    'random_state': 42\n","})\n","final_model = xgb.XGBClassifier(**best_params)\n","final_model.fit(X_train, y_train)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":1000},"collapsed":true,"id":"xkI6i3ckhBxJ","executionInfo":{"status":"ok","timestamp":1744169668183,"user_tz":-540,"elapsed":1390605,"user":{"displayName":"김현태","userId":"13921200790820738133"}},"outputId":"